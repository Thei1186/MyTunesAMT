      <TextField fx:id="txtFile" layoutX="161.0" layoutY="286.0" prefHeight="31.0" prefWidth="318.0" />
      <ComboBox fx:id="comboCategory" layoutX="161.0" layoutY="196.0" prefHeight="31.0" prefWidth="406.0" />
