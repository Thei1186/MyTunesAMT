            <Font size="29.0" />
         </font>
      </Label>
      <Label layoutX="62.0" layoutY="107.0" text="Title" />
      <Label layoutX="59.0" layoutY="155.0" text="Artist" />
      <Label layoutX="58.0" layoutY="201.0" text="Category" />
      <Label layoutX="65.0" layoutY="291.0" text="File" />
      <TextField fx:id="txtTitle" layoutX="163.0" layoutY="102.0" prefHeight="31.0" prefWidth="406.0" />
